extends layout-navigator

block links
    link(rel="stylesheet", href="css/tables.css")
    link(rel="stylesheet", href="css/repository.css")
    script(src="js/repository-in.js")

block content
    #main-container
        .alert.alert-success.invisible#alert-main
        .table-container
            table.table.table-striped
                thead
                    tr
                        th 商品编号
                        th 品牌名称
                        th 商品全名
                        th 型号
                        th 等级
                        th 规格
                        th 入库数量
                        th 成本均价
                        th 成本金额
                        th 入库时间
                        th 备注
                tbody#table-body